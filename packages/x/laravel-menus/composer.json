{
  "name": "x/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    }
  ],
  "require": {
  "php": "^8.2",
  "illuminate/support": "^10.0 || ^11.0",
  "illuminate/config": "^10.0 || ^11.0",
  "illuminate/view": "^10.0 || ^11.0"
}
,
  "require-dev": {
    "phpunit/phpunit": "~9.5",
    "mockery/mockery": "~1.5",
    "orchestra/testbench": "~7.16",
    "friendsofphp/php-cs-fixer": "^3.13"
  },
  "autoload": {
    "psr-4": {
      "X\\LaravelMenus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "X\\LaravelMenus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    },
    "laravel": {
      "providers": [
        "X\\LaravelMenus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "X\\LaravelMenus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "stable"
}
