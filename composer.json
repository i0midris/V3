{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework","laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
  {
    "type": "path",
    "url": "packages/x/laravel-menus"
  },
  {
    "type": "path",
    "url": "packages/laravelcollective/html"
  }
],

    "require": {
        "php": "^8.1.0",
        "aloha/twilio": "^4.0",
        "arcanedev/log-viewer": "^11.0",
        "automattic/woocommerce": "^3.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "composer/semver": "^3.0",
        "consoletvs/charts": "^6.8",
        "doctrine/dbal": "^3.0",
        "endroid/qr-code": "^6.0",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.7",
        "knox/pesapal": "^1.0",
        "laravel/framework": "^11.0",
        "laravel/legacy-factories": "^1.3",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "dev-main",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1.8",
        "milon/barcode": "^12.0",
        "monolog/monolog": "^3.0",
        "mpdf/mpdf": "^8.1",
        "myfatoorah/laravel-package": "^2.2",
        "nesbot/carbon": "^3.9",
        "nwidart/laravel-modules": "^10.0",
        "pusher/pusher-php-server": "^7.0.2",
        "razorpay/razorpay": "2.*",
        "spatie/flysystem-dropbox": "^3.0",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.0",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-permission": "^6.17",
        "spatie/laravel-responsecache": "^7.0",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^7.122",
        "unicodeveloper/laravel-paystack": "^1.0",
        "x/laravel-menus": "dev-main",
        "yajra/laravel-datatables-oracle": "^11.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^5.2",
        "larastan/larastan": "^3.4",
        "laravel/pint": "^1.20",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.8",
        "phpunit/phpunit": "^12.1",
        "rector/rector": "^2.0",
        "rector/rector-laravel": "^2.0",
        "tomasvotruba/bladestan": "^0.11.2"
    },
    "autoload": {
        "files": ["app/Http/helpers.php"],
        "classmap": ["database"],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Bl\\FatooraZatca\\": "Bl/FatooraZatca/src/",
            "X\\LaravelMenus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": ["php artisan key:generate"],
        "post-install-cmd": ["Illuminate\\Foundation\\ComposerScripts::postInstall"],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
"prefer-stable": true
}
